@model List<Pharm.DAL.entity.Product>

<h2>Список продуктов</h2>

<table>
    <thead>
        <tr>
            <th>Название</th>
            <th>Цена</th>
            <th>Количество</th>
            <th>Описание</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Pname</td>
                <td>@product.Price</td>
                <td>@product.Quantity</td>
                <td>@product.Description</td>
            </tr>
        }
    </tbody>
</table>
